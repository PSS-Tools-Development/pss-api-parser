[tool.poetry]
name = "pss-api-parser"
version = "0.1.0"
description = "Parses the structure of the PSS API based on flows documented through mitmproxy"
authors = [
    "The worst. <theworstpss@gmail.com>",
    "Solevis",
]
license = "MIT"
readme = "README.md"

[tool.poetry.dependencies]
python = "^3.11"
mitmproxy = "^9.0.1"
Jinja2 = "^3.1.2"
contexttimer = "^0.3.3"
colorama = "^0.4.6"

[build-system]
requires = ["poetry-core"]
build-backend = "poetry.core.masonry.api"

[tool.pytest.ini_options]
pythonpath = [
  "."
]
